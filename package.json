{
 "name": "react-component-library",
 "version": "0.0.1",
 "description": "React-component-library For used in your react Projects",
 "main": "dist/cjs/index.js",
 "module": "dist/esm/index.js",
 "types": "dist/index.d.ts",
 "files": [
  "dist"
 ],
