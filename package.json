  "build": "rollup -c",
  "dev": "rollup -c -w",
  "test:watch": "jest --watchAll --verbose",
  "test": "jest --verbose",
  "storybook": "storybook dev -p 6006",
  "build-storybook": "storybook build",
  "prepare": "husky",
  "lint": "eslint ./src/",
  "lint:fix": "eslint ./src --fix",
  "release": "standard-version"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/iamfrerot/react-component-library.git"
 },
 "keywords": [
  "react"
 ],
 "jest": {
  "testEnvironment": "jsdom"
 },
 "babel": {
  "sourceType": "unambiguous",
  "presets": [
   "@babel/preset-env",
   "@babel/preset-typescript",
   [
    "@babel/preset-react",
    {
     "runtime": "automatic"
    }
   ]
  ]
 },
